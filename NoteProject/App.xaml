<Application x:Class="NoteProject.App"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:local="clr-namespace:NoteProject"
             StartupUri="MainWindow.xaml">
    <Application.Resources>
        <SolidColorBrush x:Key="TCback" Color="LightSteelBlue" />
        <SolidColorBrush x:Key="OnlyTitle" Color="LightGray" />
        <SolidColorBrush x:Key="OnlyDate" Color="LightBlue" />
        <SolidColorBrush x:Key="Description" Color="BlanchedAlmond"/>
        <SolidColorBrush x:Key="DTframe" Color="LightCoral"/>
        <SolidColorBrush x:Key="DTback" Color="LightPink"/>
    </Application.Resources>
</Application>
